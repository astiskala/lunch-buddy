{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "version": "0.0.0",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2|ico)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "lunchmoney-api",
      "urls": [
        "https://dev.lunchmoney.app/v1/**",
        "http://localhost:3000/api/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "24h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    },
    {
      "name": "google-fonts",
      "urls": [
        "https://fonts.googleapis.com/**",
        "https://fonts.gstatic.com/**"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "365d",
        "strategy": "cacheFirst"
      }
    }
  ],
  "navigationUrls": ["/**", "!/**/*.*", "!/**/*__*", "!/**/*__*/**"],
  "navigationRequestStrategy": "performance"
}
