{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "version": "0.0.0",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/icons/**",
          "/apple-touch-icon.png",
          "/favicon.ico",
          "/icon.svg",
          "/offline.html"
        ]
      }
    },
    {
      "name": "screenshots",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": ["/screenshots/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "lunchmoney-api",
      "urls": [
        "https://api.lunchmoney.dev/**",
        "https://dev.lunchmoney.app/**",
        "http://localhost:3000/**",
        "http://localhost:4600/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "24h",
        "strategy": "performance"
      }
    },
    {
      "name": "google-fonts",
      "urls": [
        "https://fonts.googleapis.com/**",
        "https://fonts.gstatic.com/**"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "365d",
        "strategy": "performance"
      }
    }
  ],
  "navigationUrls": ["/**", "!/**/*.*", "!/**/*__*", "!/**/*__*/**"],
  "navigationRequestStrategy": "performance"
}
