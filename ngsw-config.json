{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/offline.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "lunchmoney-api",
      "urls": [
        "https://**/budgets",
        "https://**/budgets?*",
        "https://**/recurring_expenses",
        "https://**/recurring_expenses?*",
        "https://**/transactions",
        "https://**/transactions?*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "24h",
        "maxSize": 50,
        "timeout": "5s"
      }
    }
  ]
}
